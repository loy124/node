<html lang="en">

<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>

<body>
    <h3>notice list</h3>

    <table border="1">
        <thead>
            <tr>
                <th>번호</th>
                <th>작성자</th>
                <th>제목</th>
                <th>내용</th>
            </tr>
        </thead>
        <tbody>
            <% arrlist.forEach(function(list, index){
              %>
                <tr>
                    <td>
                        <%=index +1 %>
                    </td>
                    <td>
                        <a href="/detail/<%=list.WRITER %>">
                            <%= list.WRITER %>
                        </a>
                    </td>
                    <td>
                        <%=list.TITLE%>
                    </td>
                    <td>
                        <%=list.CONTENT%>
                    </td>
                </tr>
                <%
            }); %>
        </tbody>
    </table>
</body>

</html>